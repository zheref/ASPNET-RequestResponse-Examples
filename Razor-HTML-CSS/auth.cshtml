@{    
    if(IsPost)
    {
        var usuario = Request.Form["username"];
        var contrasena = Request.Form["password"];

        if(usuario == AuthData.USERNAME && contrasena == AuthData.PASSWORD)
        {
            Session["username"] = usuario;
            Session["password"] = contrasena;
            Response.Redirect("~/default");
        }
        else
        {
            Response.Redirect("~/logon");
        }
    }
}